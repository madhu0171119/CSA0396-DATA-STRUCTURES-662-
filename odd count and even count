#include<stdio.h>
int main(){
	int count_even=0;
	int count_odd=0;
	int i,n;
	int A[10];
	printf("enter the number of elements: ");
	scanf("%d",&n);
	printf("enter the elements :\n");
	for(i=1;i<=n;i++){
		scanf("%d",&A[i]);
	}
	for(i=0;i<=n;i++){
		if(A[i]%2==0){
			count_even++;
		}else{
			count_odd++;
		}
	}
	printf("even count= %d\n",count_even);
	printf("odd count= %d",count_odd);
	return 0;
}
